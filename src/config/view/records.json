        "state": {
          "label": "搜索",
          "type": "primary",
          "action": "fetch"
        }
      }
    ]
  },
  "table": {
    "columns": [
      {
        "prop": "id"
      },
      {
        "prop": "title",
        "scope": {
          "type": "Button",
          "state": {
            "value": "",
            "type": "text",
            "action": "showCreateDialog"
          }
        }
      },
      {
        "prop": "speaker_name"
      },
      {
        "prop": "speechChangci",
        "label": "场次"
      },
      {
        "prop": "release_time",
        "sortable": true
      },
      {
        "prop": "total_app_count"
      },
      {
        "prop": "total_h5_count"
      },
      {
        "prop": "total_web_count"
      },
      {
        "prop": "total_ipad_count"
      },
      {
        "prop": "is_valid",
        "scope": {
          "type": "Select",
          "state": {
            "value": "",
            "readonly": true,
            "options": [
              {
                "label": "未发布",
                "value": 0
              },
              {
                "label": "已发布",
                "value": 1
              }
            ]
          }
        }
      },
      {
        "prop": "recommended",
        "scope": {
          "type": "Select",
          "state": {
            "value": "",
            "readonly": true,
            "options": [
              {
                "label": "未推荐",
                "value": 0
              },
              {
                "label": "已推荐",
                "value": 1
              }
            ]
          }
        }
      },
      {
        "prop": "vedio",
        "scope": {
          "type": "Link",
          "state": {
            "value": ""
          }
        }
      },
      {
        "label": "操作",
        "prop": "action",
        "scope": {
          "type": "ButtonArray",
          "state": [
            {
              "label": "编辑",
              "type": "primary",
              "action": "edit"
            },
            {
              "label": "热门评论",
              "type": "primary",
              "action": "hotcomments"
            },
            {
              "label": "删除",
              "type": "danger",
              "action": "delete"
            },
            {
              "label": "上线",
              "action": "upordown"
            }
          ]
        }
      }
    ],
    "data": []
  },
  "pagination": {
    "currentPage": 1,
    "pageCount": 10,
    "total": 0,
    "background": "",
    "action": "fetch"
  },
  "dialogs": {
    "create": {
      "title": "新建",
      "items": [
        "title",
        "titlelanguage",
        {
          "prop": "speechdate",
          "type": "DatePicker",
          "state": {
            "value": "",
            "valueFormat": "yyyy-MM-dd HH:mm",
            "format": "yyyy-MM-dd HH:mm",
            "type": "datetime"
          }
        },
        {
          "prop": "release_time",
          "type": "DatePicker",
          "state": {
            "value": "",
            "valueFormat": "yyyy-MM-dd HH:mm",
            "format": "yyyy-MM-dd HH:mm",
            "type": "datetime"
          }
        },
        "speechcity",
        "keywords",
        {
          "prop": "speaker_id",
          "type": "Select",
          "state": {
            "value": "",
            "options": []
          }
        },
        {
          "prop": "draft",
          "type": "Input",
          "state": {
            "value": "",
            "type": "textarea"
          }
        },
        "additional_play_count",
        "additional_collection_count",
        "additional_share_count",
        {
          "prop": "status",
          "type": "Select",
          "state": {
            "value": "",
            "options": [
              {
                "value": 0,
                "label": "及时发布"
              },
              {
                "value": 1,
                "label": "草稿"
              }
            ]
          }
        }
      ],
      "actions": [
        {
          "label": "确定",
          "action": "create",
          "type": "primary"
        }
      ],
      "visible": false,
      "inline": true
    }
  },
  "actions": {
    "fetch": {
      "flows": [
        {
          "name": "arkfbp.flows.fetch",
          "type": "api",
          "method": "GET",
          "url": "record/",
          "request": {
            "page": "pagination.currentPage",
            "page_size": "pagination.pageCount",
            "param_title": "filter.items[prop=title].state.value",
            "param_begin_time": "filter.items[prop=begin_time].state.value",
            "param_end_time": "filter.items[prop=end_time].state.value",
            "param_recommended": "filter.items[prop=recommended].state.value",
            "param_is_valid": "filter.items[prop=is_valid].state.value"
          },
          "client_config": {
            "table.data": "data.items",
            "pagination.total": "data.total"
          }
        },
        {
          "name": "flows.spliceValue",
          "request": "table.data"
        }
      ]
    },
    "showCreateDialog": {
      "flows": [
        {
          "name": "arkfbp.flows.assign",
          "type": "assign",
          "client_config": {
            "dialogs.create.visible": true
          }
        }
      ]
    }
  }
}