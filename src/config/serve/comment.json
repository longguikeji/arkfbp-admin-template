{
  "comment": {
    "name": "comment",
    "type": "table",
    "meta": {
      "items": {
        "title": "评论列表",
        "type": {
          "array": {
            "array_item": "comment"
          }
        }
      },
      "data": {
        "required": false,
        "type": {
          "object": {
            "total": "pagination.total",
            "page": "pagination.page",
            "page_size": "pagination.page_size",
            "items": "items"
          }
        }
      },
      "comment": {
        "title": "返回的评论列表",
        "required": true,
        "type": {
          "object": {
            "id": "model_comment.id",
            "nickname": "model_comment.nickname",
            "content": "model_comment.content",
            "title": "model_comment.title", 
            "is_selected": "model_comment.is_selected",
            "created": "model_comment.created",
            "begin_time": "model_comment.begin_time",
            "end_time": "model_comment.end_time",
            "mobile": "model_comment.mobile"
          }
        }
      }
    },
    "api": {
      "comment/": {
        "get": {
          "pagination": true,
          "name": "价格列表获取",
          "type": "retrieve",
          "request": {
            "content": {
              "src": "model_comment.content"
            },
            "begin_time": {
              "src": "model_comment.begin_time"
            },
            "end_time": {
              "src": "model_comment.end_time"
            },
            "mobile": {
              "src": "model_comment.mobile"
            }
          }
        }
      }
    }
  },
  "pagination": {
    "name": "pagination",
    "type": "",
    "meta": {
      "page": {
        "title": "页码",
        "type": {
          "integer": {}
        }
      },
      "page_size": {
        "title": "页面大小",
        "type": {
          "integer": {
            "default": 20,
            "required": false
          }
        }
      },
      "total": {
        "title": "总数",
        "type": {
          "integer": {}
        }
      },
      "count": {
        "title": "分页总数",
        "type": {
          "integer": {}
        }
      },
      "next": {
        "title": "下一页地址",
        "type": {
          "string": {
            "allow_null": true
          }
        }
      },
      "previous": {
        "title": "前一页地址",
        "type": {
          "string": {
            "allow_null": true
          }
        }
      },
      "item": {
        "title": "列表元素",
        "type": {
          "any": {}
        }
      },
      "results": {
        "title": "查询结果列表",
        "type": {
          "array": {
            "array_item": "item"
          }
        }
      }
    },
    "api": {}
  },
  "model_comment": {
    "name": "model_comment",
    "type": "",
    "meta": {
      "id": {
        "title": "#",
        "type": {
          "string": {}
        }
      },
      "title": {
        "title": "标题",
        "type": {
          "string": {}
        }
      },
      "content": {
        "title": "内容",
        "type": {
          "string": {}
        }
      },
      "begin_time": {
        "title": "开始时间",
        "type": {
          "string": {}
        }
      },
      "end_time": {
        "title": "结束时间",
        "type": {
          "string": {}
        }
      },
      "mobile": {
        "title": "手机",
        "type": {
          "string": {}
        }
      },
      "nickname": {
        "title": "昵称",
        "type": {
          "string": {}
        }
      },
      "is_selected": {
        "title": "精选状态",
        "type": {
          "string": {}
        }
      },
      "created": {
        "title":"创建时间",
        "type": {
          "string": {}
        }
      }
    },
    "api": {}
  }
}