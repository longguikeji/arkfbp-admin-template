{
  "speaker": {
    "name": "speaker",
    "type": "table",
    "meta": {
      "items": {
        "title": "查询结果列表",
        "type": {
          "array": {
            "array_item": "speaker"
          }
        }
      },
      "data": {
        "required": false,
        "type": {
          "object": {
            "total": "pagination.total",
            "page": "pagination.page",
            "page_size": "pagination.page_size",
            "items": "items"
          }
        }
      },
      "speaker": {
        "title": "用于返回的讲者信息",
        "required": true,
        "type": {
          "object": {
            "id": "model_speaker.id"
         
          }
        }
      }
    },
    "api": {
      "speaker/": {
        "post": {
          "name": "新建讲者",
          "type": "create",
          "request": {
            "name": {
              "src": "model_speaker.name"
            }
            
          },
          "response": {
            "speaker": "speaker"
          },
          "debug": false
        },
        "get": {
          "pagination": true,
          "name": "查询讲者列表",
          "type": "retrieve",
          "request": {
            "name": {
              "src": "model_speaker.name"
            }
          },
          "response": {
            "data": "data"
          },
          "debug": false
        }
      },
      "speaker/<id>/": {
        "index": {
          "id": {
            "src": "model_speaker.id"
          }
        },
        "patch": {
          "name": "更新讲者",
          "type": "update",
          "request": {
            "name": {
               "src": "model_speaker.name"
            }
          },
          "response": {
            "data": "data"
          }
        },
        "delete": {
          "name": "删除讲者",
          "type": "delete",
          "request": {}
        }
      }
    }
  },
  "pagination": {
    "name": "pagination",
    "type": "",
    "meta": {
      "page": {
        "title": "页码",
        "type": {
          "integer": {}
        }
      },
      "page_size": {
        "title": "页面大小",
        "type": {
          "integer": {
            "default": 20,
            "required": false
          }
        }
      },
      "total": {
        "title": "总数",
        "type": {
          "integer": {}
        }
      },
      "count": {
        "title": "分页总数",
        "type": {
          "integer": {}
        }
      },
      "next": {
        "title": "下一页地址",
        "type": {
          "string": {
            "allow_null": true
          }
        }
      },
      "previous": {
        "title": "前一页地址",
        "type": {
          "string": {
            "allow_null": true
          }
        }
      },
      "item": {
        "title": "列表元素",
        "type": {
          "any": {}
        }
      },
      "results": {
        "title": "查询结果列表",
        "type": {
          "array": {
            "array_item": "item"
          }
        }
      }
    },
    "api": {}
  },
  "model_speaker": {
    "name": "model_speaker",
    "type": "",
    "meta": {
      "id": {
        "title": "讲者ID",
        "type": {
          "string": {}
        }
      }
   
    },
    "api": {}
  }
}
