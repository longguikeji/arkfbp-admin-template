            },
            "video_cover": {
              "src": "model_records.video_cover"
            },
            "speaker_id": {
              "src": "model_records.speaker_id"
            }
          },
          "response": {
            "records": "records"
          }
        }
      },
      "record/<id>/edit/": {
        "post": {
          "pagination": false,
          "name": "编辑记录",
          "type": "update",
          "request": {
            "titlelanguage": {
