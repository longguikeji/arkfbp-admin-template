{
  "sence": {
    "name": "sence",
    "type": "table",
    "meta": {
      "items": {
        "title": "评论列表",
        "type": {
          "array": {
            "array_item": "sence"
          }
        }
      },
      "data": {
        "required": false,
        "type": {
          "object": {
            "total": "pagination.total",
            "page": "pagination.page",
            "page_size": "pagination.page_size",
            "items": "items"
          }
        }
      },
      "sence": {
        "title": "返回的评论列表",
        "required": true,
        "type": {
          "object": {
            "id": "model_sence.id",
            "title": "model_sence.title",
            "is_valid": "model_sence.is_valid",
            "activity_type": "model_sence.activity_type", 
            "recommended": "model_sence.recommended",
            "activity_date_sort": "model_sence.activity_date_sort",
            "created_sort": "model_sence.created_sort",
            "status": "model_sence.status",
            "city": "model_sence.city",
            "video": "model_sence.video",
            "periods": "model_sence.periods",
            "date": "model_sence.date",
            "address": "model_sence.address"
          }
        }
      }
    },
    "api": {
      "activity/": {
        "get": {
          "pagination": true,
          "name": "价格列表获取",
          "type": "retrieve",
          "request": {
            "title": {
              "src": "model_sence.title"
            },
            "is_valid": {
              "src": "model_sence.is_valid"
            },
            "activity_type": {
              "src": "model_sence.activity_type"
            },
            "recommended": {
              "src": "model_sence.recommended"
            },
            "activity_date_sort": {
              "src": "model_sence.activity_date_sort"
            },
            "created_sort": {
              "src": "model_sence.created_sort"
            }
          }
        }
      }
    }
  },
  "pagination": {
    "name": "pagination",
    "type": "",
    "meta": {
      "page": {
        "title": "页码",
        "type": {
          "integer": {}
        }
      },
      "page_size": {
        "title": "页面大小",
        "type": {
          "integer": {
            "default": 20,
            "required": false
          }
        }
      },
      "total": {
        "title": "总数",
        "type": {
          "integer": {}
        }
      },
      "count": {
        "title": "分页总数",
        "type": {
          "integer": {}
        }
      },
      "next": {
        "title": "下一页地址",
        "type": {
          "string": {
            "allow_null": true
          }
        }
      },
      "previous": {
        "title": "前一页地址",
        "type": {
          "string": {
            "allow_null": true
          }
        }
      },
      "item": {
        "title": "列表元素",
        "type": {
          "any": {}
        }
      },
      "results": {
        "title": "查询结果列表",
        "type": {
          "array": {
            "array_item": "item"
          }
        }
      }
    },
    "api": {}
  },
  "model_sence": {
    "name": "model_sence",
    "type": "",
    "meta": {
      "id": {
        "title": "#",
        "type": {
          "integer": {}
        }
      },
      "title": {
        "title": "标题",
        "type": {
          "string": {}
        }
      },
      "is_valid": {
        "title": "状态",
        "type": {
          "string": {}
        }
      },
      "activity_type": {
        "title": "现场类型",
        "type": {
          "integer": {}
        }
      },
      "recommended": {
        "title": "是否推荐",
        "type": {
          "integer": {}
        }
      },
      "activity_date_sort": {
        "title": "举办时间",
        "type": {
          "integer": {}
        }
      },
      "created_sort": {
        "title": "添加时间",
        "type": {
          "integer": {}
        }
      },
      "status": {
        "title": "活动状态",
        "type": {
          "integer": {}
        }
      },
      "city": {
        "title":"城市",
        "type": {
          "string": {}
        }
      },
      "video": {
        "title":"花絮",
        "type": {
          "string": {}
        }
      },
      "periods": {
        "title":"期数",
        "type": {
          "integer": {}
        }
      },
      "date": {
        "title":"日期",
        "type": {
          "string": {}
        }
      },
      "address": {
        "title":"地址",
        "type": {
          "string": {}
        }
      },
      "created": {
        "title":"创建时间",
        "type": {
          "string": {}
        }
      }
    },
    "api": {}
  }
}